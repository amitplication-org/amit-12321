datasource postgres {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  createdAt    DateTime    @default(now())
  firstName    String?
  id           String      @id @default(cuid())
  lastName     String?
  newEntity    NewEntity[]
  password     String
  roles        Json
  serialNumber String?
  someField    String?
  updatedAt    DateTime    @updatedAt
  username     String      @unique
}

model NewEntity {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
  users     User?    @relation(fields: [usersId], references: [id])
  usersId   String?
}
